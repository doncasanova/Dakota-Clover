extends layout

block content
		<div class="text-centered">
			h1.text-white All events.
		</div>
			each value in allEvents
				
				<div class="row">
				<div class="col-3 border m-1">
					<p class="text-white"> #{value.id}</p>
					<p class="text-white"> #{value.name}</p>
				</div>
				<div class="col-3">
					<button id=" #{value.id}" class="text-danger" >Delete</button>
					<button id=" #{value.id}" class="text-dark">Update</button>
				</div>
				</div>